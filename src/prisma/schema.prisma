generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Account {
  id                   String  @id @default(uuid())
  email                String  @unique
  attestationChallenge String?
  assertionChallenge   String?
  credentialPublicKey  String?
  authCounter          Int     @default(0)
}
