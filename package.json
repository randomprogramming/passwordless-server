{
  "name": "passwordless-server",
  "version": "1.0.0",
  "description": "Passwordless Service Server",
  "main": "index.ts",
  "repository": "https://github.com/randomprogramming/passwordless-server",
  "author": "Dominik <dgoluban38@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "prisma:migrate": "prisma migrate dev --preview-feature",
    "prisma:format": "prisma format"
  },
  "nodemonConfig": {
    "watch": [
      "index.ts",
      "src",
      ".env"
    ],
    "ext": "ts, json, env",
    "exec": "ts-node --transpile-only index.ts"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.31",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "prisma": "^3.13.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@prisma/client": "^3.13.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "fido2-lib": "^2.8.3",
    "joi": "^17.6.0"
  }
}
